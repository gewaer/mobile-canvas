{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "jsx-boolean-value": false,
    "max-line-length": [
      true,
      { "limit": 120, "ignore-pattern": "^import |^export {(.*?)}" }
    ]
  }
}
